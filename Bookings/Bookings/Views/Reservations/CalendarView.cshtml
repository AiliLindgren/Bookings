@model Bookings.Models.ViewModels.CalendarDayVM[]
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewBag.Title = "Booking";
    ViewBag.Header = "Booking";
}

<!DOCTYPE html>


<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CalendarView</title>
</head>
<body>
    @foreach (var item in Model)
    {
        <p>Weekend: @item.IsWeekend Full? @item.IsFull</p>

        @foreach (var slot in item.CalendarTimeSlots)
        {
            <a>Start time: @slot.Start End time: @slot.End</a>
        }
    }
</body>
</html>
