@model Bookings.Models.ViewModels.ReservationsIndexVM[]
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h3>Reservations</h3>
    <ul>
        @foreach (var item in Model)
        {
        <li>Booking for: @item.NumberOfPeople Date: @item.Date Time: @item.StartTime Your contact: @item.Contact Has space: @if (item.HasSpace) { <b>Yes</b> } </li>
          
        }
    </ul>
</body>
</html>